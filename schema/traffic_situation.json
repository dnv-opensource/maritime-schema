                    "title": "Conditions"
                }
            },
            "title": "Environment",
            "type": "object"
        },
        "GeoPosition": {
            "additionalProperties": true,
            "properties": {
                "lon": {
                    "description": "WGS-84 Longitude",
                    "examples": [
                        51.2131
                    ],
                    "maximum": 180.0,
                    "minimum": -180.0,
                    "title": "Lon",
                    "type": "number"
                },
                "lat": {
                    "description": "WGS-84 Latitude",
                    "examples": [
                        51.2131
                    ],
                    "maximum": 90.0,
                    "minimum": -90.0,
                    "title": "Lat",
                    "type": "number"
                }
