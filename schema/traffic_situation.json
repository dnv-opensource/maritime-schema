            "title": "PrecipitationType",
            "type": "string"
        },
        "RouteData": {
            "additionalProperties": {
                "$ref": "#/$defs/DataPoint"
            },
            "title": "RouteData",
            "type": "object"
        },
        "ShipState": {
            "description": "The current state of a ship.",
            "properties": {
                "position": {
                    "$ref": "#/$defs/GeoPosition",
                    "description": "longitude and latitude of the ship.",
                    "examples": [
                        {
                            "lat": 10.3432,
                            "lon": 57.2343
                        }
                    ],
                    "title": "Position"
                },
                "sog": {
                    "description": "ship  (SOG) ground in knots",
                    "examples": [
                        10.0
                    ],
                    "minimum": 0.0,
                    "title": "Sog",
                    "type": "number"
                },
                "cog": {
                    "description": "ship course over ground (COG) in degrees",
                    "examples": [
                        45.0
                    ],
                    "maximum": 360.0,
                    "minimum": 0.0,
                    "title": "Cog",
                    "type": "number"
                },
                "heading": {
                    "description": "ship heading in degrees",
                    "examples": [
                        45.2
                    ],
                    "maximum": 360.0,
                    "minimum": 0.0,
                    "title": "Heading",
                    "type": "number"
                },
                "navStatus": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/AisNavStatus"
                        },
                        {
                            "$ref": "#/$defs/AisNavStatusInt"
                        }
                    ],
                    "description": "AIS Navigational Status",
                    "title": "Nav Status"
                }
            },
            "required": [
                "position",
                "sog",
                "cog"
            ],
            "title": "ShipState",
            "type": "object"
        },
        "ShipStatic": {
            "additionalProperties": true,
            "description": "Static ship data that will not change during the scenario.",
            "properties": {
                "id": {
                    "description": "Ship Identifier",
                    "examples": [
                        1
                    ],
                    "maximum": 4294967296,
                    "minimum": 0,
                    "title": "Id",
                    "type": "integer"
                },
                "mmsi": {
                    "description": "Maritime Mobile Service Identity (MMSI)",
                    "examples": [
                        123456789
                    ],
                    "maximum": 999999999,
                    "minimum": 100000000,
                    "title": "Mmsi",
                    "type": "integer"
                },
                "imo": {
                    "description": "IMO Number",
                    "examples": [
                        1234567
                    ],
                    "maximum": 9999999,
                    "minimum": 1000000,
                    "title": "Imo",
                    "type": "integer"
                },
                "name": {
                    "description": "Ship name",
                    "examples": [
                        "RMS Titanic"
                    ],
                    "title": "Name",
                    "type": "string"
                },
                "dimensions": {
                    "$ref": "#/$defs/Dimensions",
                    "description": "Key ship dimensions"
                },
                "shipType": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/AisShipType"
                        },
                        {
                            "$ref": "#/$defs/AisShipAndCargoType"
                        },
                        {
                            "$ref": "#/$defs/AisShipAndCargoTypeInt"
                        }
                    ],
                    "description": "General ship type, based on AIS",
                    "title": "Ship Type"
                },
                "stationType": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/AisStationType"
                        },
                        {
                            "$ref": "#/$defs/AisStationTypeInt"
                        }
                    ],
                    "description": "AIS station type",
                    "title": "Station Type"
                },
                "positionFixingDevice": {
                    "anyOf": [
                        {
                            "$ref": "#/$defs/AisPositionFixingDevice"
                        },
                        {
                            "$ref": "#/$defs/AisPositionFixingDeviceInt"
                        }
                    ],
                    "description": "AIS position fixing device",
                    "title": "Position Fixing Device"
                },
                "pathType": {
                    "allOf": [
                        {
                            "$ref": "#/$defs/PathType",
                            "description": "Specifies the control-point model (e.g., Bezier, RTZ) used to define the path for the ship to follow."
                        }
                    ],
                    "default": "rtz",
                    "title": "Path Type"
                },
                "initDelay": {
                    "description": "Delay in seconds, before the ship is initialized",
                    "examples": [
                        15.0
                    ],
                    "minimum": 0.0,
                    "title": "Init Delay",
                    "type": "number"
                },
                "sogMin": {
                    "description": "Minimum ship speed over ground in knots",
                    "examples": [
                        5.0
                    ],
                    "minimum": 0.0,
                    "title": "Sog Min",
                    "type": "number"
                },
                "sogMax": {
                    "description": "Maximum ship speed over ground in knots",
                    "examples": [
                        15.0
                    ],
                    "minimum": 0.0,
                    "title": "Sog Max",
                    "type": "number"
                }
            },
            "required": [
                "id"
            ],
            "title": "ShipStatic",
            "type": "object"
        },
        "TargetShip": {
            "properties": {
                "initial": {
